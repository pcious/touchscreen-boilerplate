{
  "name": "touchscreen-boilerplate",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "electronScripts/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run clean && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "./node_modules/.bin/eslint --fix src",
    "prepush": "npm run lint",
    "clean": "rimraf build",
    "dev": "./node_modules/.bin/concurrently \"npm run dev:start\" \"./node_modules/.bin/wait-on http://localhost:3000/ && npm run electron:start\"",
    "prd": "npm run build && ELECTRON_DEV=true npm run electron",
    "elec": "ELECTRON_DEV=true npm run electron",
    "package": "npm run build && node ./electronScripts/build.js",
    "package-osx": "npm run build && node ./electronScripts/build-mac.js",
    "electron": "./node_modules/.bin/electron .",
    "dev:start": "BROWSER=none ./node_modules/.bin/react-scripts start",
    "electron:start": "cross-env ELECTRON_DEV=true ELECTRON_START_URL=http://localhost:3000 npm run electron"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "./node_modules/.bin/eslint --fix src",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.1",
    "@storybook/addon-links": "^4.0.0-alpha.1",
    "@storybook/addons": "^4.0.0-alpha.1",
    "@storybook/react": "^4.0.0-alpha.1",
    "electron": "^2.0.0",
    "electron-packager": "^12.0.2",
    "fs-extra": "^6.0.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "keen-tracking": "^1.4.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-modal-es": "^1.2.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-router-transition": "^1.2.1",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-modux": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "husky": "^0.14.3",
    "rimraf": "^2.6.2",
    "wait-on": "^2.1.0"
  }
}
